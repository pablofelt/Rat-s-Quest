// room script file

function room_LeaveRight()
{
  player.ChangeRoomAutoPosition(13);
}

function room_LeaveLeft()
{
  if (player.y>360){
    player.ChangeRoom(15, 300, 580);
  }
  else{
    player.ChangeRoom(15, 780, 385);
  }
}

function room_LeaveBottom()
{
  player.ChangeRoom(16, 800-player.x, 580);
}
