// room script file

function room_LeaveRight()
{
  player.ChangeRoomAutoPosition(8);
}

function room_LeaveLeft()
{
  player.ChangeRoomAutoPosition(10);
}

function room_LeaveTop()
{
  if (player.x < 200){
    // brick table
    player.ChangeRoom(22, 780, 400);
  }
  else{
    // lichfield lawn
    player.ChangeRoom(23, 734, 248);
  }
}
